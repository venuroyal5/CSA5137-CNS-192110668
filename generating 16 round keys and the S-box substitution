// Function to perform permutation using a permutation table
void permutation(int *table, int size, uint64_t input, uint64_t *output) {
 *output = 0;
 for (int i = 0; i < size; i++) {
 *output <<= 1;
 *output |= (input >> (64 - table[i])) & 1;
 }
}
